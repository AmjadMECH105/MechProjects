% Given experimental data
t_exp = 10:10:60;
c_exp = [3.4 2.6 1.6 1.3 1.0 0.5];

% Expected function
t_func = [1 141];
c_func = [1 141];

% Plot
plot(t_exp,c_exp,'rd');
%xlabel, ylabel, and legend
xlabel=('Time (m)');
ylabel=('Concentration (ppm)');
legend=('Concentration');
hold on
plot(t_func,c_func,'g--');
hold off
xlabel=('Time (m)');
ylabel=('Concentration (ppm)');
legend=('Concentration');
